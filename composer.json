{
  "name": "codwelt/carbon-colombia",
  "description": "Calculo de dias laborales, hábiles y calendario según el calendario colombiano de dias festivos y fines de semana",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "codwelt",
    "carbon-colombia",
    "carbon-dias-laborales"
  ],
  "homepage": "https://github.com/codwelt/carbon-dias-laborales-colombia",
  "autoload": {
    "psr-4": {
      "Codwelt\\CarbonColombia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codwelt\\CarbonColombia\\Tests\\": "test/"
    }
  },
  "authors": [
    {
      "name": "webdev",
      "email": "webdev@codwelt.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "nesbot/carbon": "3.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "10.2.x-dev"
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  }
}
